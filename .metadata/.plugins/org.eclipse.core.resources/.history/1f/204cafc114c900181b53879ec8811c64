package RangeQuery.IndexTree;

import java.util.Scanner;

public class PURQ {
	public static Scanner scn = new Scanner(System.in);

	private static int size;
	private static long[] ftree;

	public static void main(String[] args) throws Exception {
		solve();
	}

	public static void solve() throws Exception {

		ftree = new long[size];
	}

	public static void fenwickTree(int[] arr) {

	}

	public static void query(int data, int index, long[] ftree, int size) {
		index++;
		long sum = 0;
		while (index < size) {
			sum += ftree[index];
			index += (index & -index);
		}
	}

	private static void update(int data, int index) {

	}

	private static int[] ArrayInput(int n) throws IOException {
		int[] arr = new int[n];
		for (int i = 0; i < n; i++) {
			arr[i] = scn.nextInt();
		}
		return arr;
	}

}
